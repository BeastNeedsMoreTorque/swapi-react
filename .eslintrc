{
  "extends": "airbnb",
  "plugins": [
    "babel",
    "jsx-a11y",
    "react",
    "import"
  ],
  "rules": {
    "no-underscore-dangle": 0,
    "react/require-extension": "off"
  },
  "ecmaFeatures": {
    "modules": true
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "mocha": true
  }
}
